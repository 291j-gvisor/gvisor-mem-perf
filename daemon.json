{
    "runtimes": {
        "runsc": {
            "path": "/usr/local/bin/runsc"
        },
        "runsc-debug": {
            "path": "/usr/local/bin/runsc",
            "runtimeArgs": [
                "--debug-log=/tmp/runsc",
                "--debug",
                "--strace"
            ]
        },
        "runsc-prof": {
            "path": "/usr/local/bin/runsc",
            "runtimeArgs": [
                "--profile"
            ]
        },
        "runsc-dev": {
            "path": "/usr/local/bin/runsc-dev"
        },
        "runsc-dev-debug": {
            "path": "/usr/local/bin/runsc-dev",
            "runtimeArgs": [
                "--debug-log=/tmp/runsc-dev/",
                "--debug",
                "--strace"
            ]
        },
        "runsc-dev-prof": {
            "path": "/usr/local/bin/runsc-dev",
            "runtimeArgs": [
                "--profile"
            ]
        }
    }
}
